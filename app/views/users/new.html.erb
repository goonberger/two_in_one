<% if @user %>
  <% @user.errors.full_messages.each %>
<% end %>

<% flash.each do |_, message| %>
  <%= message %>
<% end %>

<%= form_with url: register_path do |f| %>
  <p><%= f.label :name %><%= f.text_field :name %></p>
  <p><%= f.label :email %><%= f.email_field :email %></p>
  <p><%= f.label :house_number %><%= f.text_field :house_number %></p>
  <p><%= f.label :street %><%= f.text_field :street %></p>
  <p><%= f.label :city %><%= f.text_field :city %></p>
  <p><%= f.submit 'Register' %></p>
<% end %>